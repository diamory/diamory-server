    "@typescript-eslint/parser": "^5.10.2",
    "assertthat": "6.5.2",
    "aws-sdk": "2.1314.0",
    "esbuild": "^0.14.14",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.1",
    "js-yaml": "4.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
