{
  "name": "diamory-server",
  "version": "0.1.0",
  "description": "Backend source code (AWS SAM Application) for diamory.",
  "license": "MIT",
  "author": "Mark Herrmann",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diamory/diamory-browser.git"
  },
  "private": true,
  "scripts": {
    "compile_": "tsc",
    "unit_": "jest",
    "lint": "eslint '*.ts' --quiet --fix",
    "test": "./scripts/tests.sh",
    "deploy": "sam build && sam deploy --profile diamory",
    "deploy:guided": "sam build && sam deploy --guided --profile diamory",
    "drop-stack-yes-i-really-know-what-i-am-doing": "aws cloudformation delete-stack --stack-name diamory-server --profile diamory"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.271.0",
    "@aws-sdk/lib-dynamodb": "3.271.0",
    "@types/aws-lambda": "8.10.110",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "assertthat": "6.5.2",
    "esbuild": "0.17.8",
    "esbuild-jest": "0.5.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.4.2",
    "js-yaml": "4.1.0",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
